{
  "name": "strange-middle-end",
  "version": "0.4.0",
  "description": "a middle-end composer using redux, normalizr, and immer",
  "main": "dist/strange-middle-end.js",
  "module": "dist/strange-middle-end.module.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "npm run clean && rollup -c",
    "clean": "rimraf dist",
    "test": "lab -a @hapi/code -t 100 -L -I 'regeneratorRuntime'",
    "lint": "lab -dL -I 'regeneratorRuntime'",
    "coveralls": "lab -r lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bigroomstudios/strange-middle-end.git"
  },
  "keywords": [
    "middle-end",
    "composer",
    "utility",
    "redux",
    "normalizr",
    "immer",
    "react"
  ],
  "author": "Devin Ivy <devin@bigroomstudios.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bigroomstudios/strange-middle-end/issues"
  },
  "homepage": "https://github.com/bigroomstudios/strange-middle-end#readme",
  "peerDependencies": {
    "redux": ">=3 <5",
    "redux-thunk": "2.x.x",
    "immer": ">=1 <9",
    "normalizr": "3.x.x",
    "react": ">=16.8.x <18"
  },
  "devDependencies": {
    "@babel/core": "7.x.x",
    "@babel/preset-env": "7.x.x",
    "@hapi/code": "8.x.x",
    "@hapi/lab": "22.x.x",
    "@rollup/plugin-node-resolve": "7.x.x",
    "@testing-library/react-hooks": "3.x.x",
    "coveralls": "3.x.x",
    "immer": "8.x.x",
    "normalizr": "3.x.x",
    "npm-run-all": "4.x.x",
    "react": "17.x.x",
    "react-test-renderer": "16.8.x",
    "redux": "4.x.x",
    "redux-thunk": "2.x.x",
    "rimraf": "3.x.x",
    "rollup": "2.x.x",
    "rollup-plugin-babel": "4.x.x",
    "rollup-plugin-cjs-es": "1.x.x",
    "rollup-plugin-filesize": "7.x.x",
    "rollup-plugin-peer-deps-external": "2.x.x",
    "rollup-plugin-terser": "5.x.x"
  }
}
